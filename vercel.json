{
  "rewrites": [
    {
      "source": "/",
      "destination": "/static/index.html"
    },
    {
      "source": "/static/:path*",
      "destination": "/static/:path*"
    },
    {
      "source": "/app/:path*",
      "has": [
        {
          "type": "host",
          "value": "(?<host>.*)"
        }
      ],
      "destination": "/app/:path*"
    }
  ],
  "buildCommand": "next build",
  "outputDirectory": ".next",
  "framework": "nextjs",
  "regions": ["hnd1"],
  "env": {
    "NEXTAUTH_URL": "https://${VERCEL_URL}",
    "NEXTAUTH_SECRET": "${NEXTAUTH_SECRET}"
  }
}
